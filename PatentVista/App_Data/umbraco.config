        <Artikel id="4663" parentID="4386" level="4" creatorID="0" sortOrder="2" createDate="2013-11-22T09:22:43" updateDate="2014-01-21T15:21:17" nodeName="International Patent Applications PCT" urlName="international-patent-applications-pct" path="-1,4382,4383,4386,4663" isDoc="" nodeType="4619" creatorName="Ruben" writerName="Ruben" writerID="0" template="4616" nodeTypeAlias="Artikel">
