      <Rubriek id="4630" parentID="4383" level="3" creatorID="0" sortOrder="0" createDate="2013-11-20T20:49:39" updateDate="2014-01-15T21:14:16" nodeName="Intellectual Property Rights" urlName="intellectual-property-rights" path="-1,4382,4383,4630" isDoc="" nodeType="4385" creatorName="Ruben" writerName="Ruben" writerID="0" template="4617" nodeTypeAlias="Rubriek">
