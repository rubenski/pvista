        <Artikel id="4707" parentID="4660" level="4" creatorID="0" sortOrder="2" createDate="2013-12-12T19:33:19" updateDate="2014-01-15T21:14:36" nodeName="Alternatieve Brandstoffen" urlName="alternatieve-brandstoffen" path="-1,4388,4636,4660,4707" isDoc="" nodeType="4619" creatorName="Ruben" writerName="Ruben" writerID="0" template="4616" nodeTypeAlias="Artikel">
