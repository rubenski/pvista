        <Artikel id="4650" parentID="4646" level="4" creatorID="0" sortOrder="1" createDate="2013-11-22T08:51:22" updateDate="2014-01-15T21:14:35" nodeName="Europese Aanvraag" urlName="europese-aanvraag" path="-1,4388,4636,4646,4650" isDoc="" nodeType="4619" creatorName="Ruben" writerName="Ruben" writerID="0" template="4616" nodeTypeAlias="Artikel">
