      <Rubriek id="4637" parentID="4636" level="3" creatorID="0" sortOrder="0" createDate="2013-11-21T15:41:15" updateDate="2014-01-15T21:14:35" nodeName="Intellectueel Eigendom" urlName="intellectueel-eigendom" path="-1,4388,4636,4637" isDoc="" nodeType="4385" creatorName="Ruben" writerName="Ruben" writerID="0" template="4617" nodeTypeAlias="Rubriek">
