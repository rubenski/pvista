        <Artikel id="4706" parentID="4646" level="4" creatorID="0" sortOrder="10" createDate="2013-12-12T18:28:11" updateDate="2014-08-10T15:14:13" nodeName="Octrooi aanvragen in India" urlName="octrooi-aanvragen-in-india" path="-1,4388,4636,4646,4706" isDoc="" nodeType="4619" creatorName="Ruben" writerName="Ruben" writerID="0" template="4616" nodeTypeAlias="Artikel">
