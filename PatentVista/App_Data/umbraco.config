      <Rubriek id="4646" parentID="4636" level="3" creatorID="0" sortOrder="1" createDate="2013-11-22T08:48:00" updateDate="2014-01-15T21:14:35" nodeName="Octrooi Aanvragen" urlName="octrooi-aanvragen" path="-1,4388,4636,4646" isDoc="" nodeType="4385" creatorName="Ruben" writerName="Ruben" writerID="0" template="4617" nodeTypeAlias="Rubriek">
