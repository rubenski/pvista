        <Artikel id="4649" parentID="4646" level="4" creatorID="0" sortOrder="2" createDate="2013-11-22T08:50:53" updateDate="2014-08-09T08:25:48" nodeName="Internationale Aanvraag PCT" urlName="internationale-aanvraag-pct" path="-1,4388,4636,4646,4649" isDoc="" nodeType="4619" creatorName="Ruben" writerName="Ruben" writerID="0" template="4616" nodeTypeAlias="Artikel">
