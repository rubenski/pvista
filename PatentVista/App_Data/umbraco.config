        <Artikel id="4651" parentID="4646" level="4" creatorID="0" sortOrder="3" createDate="2013-11-22T08:51:51" updateDate="2014-08-08T18:19:20" nodeName="Regionale Aanvraag" urlName="regionale-aanvraag" path="-1,4388,4636,4646,4651" isDoc="" nodeType="4619" creatorName="Ruben" writerName="Ruben" writerID="0" template="4616" nodeTypeAlias="Artikel">
