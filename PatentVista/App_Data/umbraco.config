        <Artikel id="4653" parentID="4646" level="4" creatorID="0" sortOrder="4" createDate="2013-11-22T08:58:05" updateDate="2014-01-15T21:14:36" nodeName="Triade Octrooien" urlName="triade-octrooien" path="-1,4388,4636,4646,4653" isDoc="" nodeType="4619" creatorName="Ruben" writerName="Ruben" writerID="0" template="4616" nodeTypeAlias="Artikel">
