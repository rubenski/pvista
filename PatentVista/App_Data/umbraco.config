        <Artikel id="4708" parentID="4660" level="4" creatorID="0" sortOrder="3" createDate="2013-12-12T19:41:04" updateDate="2014-01-15T21:14:36" nodeName="Brandstofcellen" urlName="brandstofcellen" path="-1,4388,4636,4660,4708" isDoc="" nodeType="4619" creatorName="Ruben" writerName="Ruben" writerID="0" template="4616" nodeTypeAlias="Artikel">
