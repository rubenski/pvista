        <Artikel id="4701" parentID="4386" level="4" creatorID="0" sortOrder="5" createDate="2013-12-12T18:23:02" updateDate="2014-01-15T21:14:16" nodeName="EU Community Patent" urlName="eu-community-patent" path="-1,4382,4383,4386,4701" isDoc="" nodeType="4619" creatorName="Ruben" writerName="Ruben" writerID="0" template="4616" nodeTypeAlias="Artikel">
