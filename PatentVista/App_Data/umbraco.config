        <Artikel id="4738" parentID="4386" level="4" creatorID="0" sortOrder="11" createDate="2014-01-02T19:08:32" updateDate="2014-01-15T21:14:16" nodeName="US Patent Applications" urlName="us-patent-applications" path="-1,4382,4383,4386,4738" isDoc="" nodeType="4619" creatorName="Ruben" writerName="Ruben" writerID="0" template="4616" nodeTypeAlias="Artikel">
